# ===== BloFin (example) =====
BLOFIN_BASE_URL=https://openapi.blofin.com
BLOFIN_WS_PRIVATE=wss://openapi.blofin.com/ws/private
BLOFIN_WS_PUBLIC=wss://openapi.blofin.com/ws/public
# Fill these in only in .env.local (not tracked)
BLOFIN_API_KEY=
BLOFIN_API_SECRET=
BLOFIN_API_PASSPHRASE=

########################################
## ====== Scanner Settings ====== ##
########################################
SCAN_TIMEFRAMES=1m,5m,15m,1H
CANDLES_LIMIT=160
TOP_SYMBOLS=200
SCORE_MIN=6.0
SCORE_MAX=7.0

########################################
## ====== Liquidity / Filters ====== ##
########################################
MIN_QUOTE_VOL_USDT=400000
MIN_LAST_PRICE=
INCLUDE_SYMBOLS=

########################################
## ====== Dedupe / Cooldowns ====== ##
########################################
SIGNAL_COOLDOWN_SEC=90
DEDUPE_TTL_SEC=600

########################################
## ====== Request Pacing (REST) ===== ##
########################################
BLOFIN_MIN_REQ_INTERVAL_MS=400
BLOFIN_429_COOLDOWN_SEC=150
CANDLES_CONCURRENCY=15
CANDLES_CHUNK=24
CANDLES_INTER_CHUNK_SLEEP=0.6
TF_SLEEP_SEC=0.8
LOOP_SLEEP_SEC=8

########################################
## ====== WebSocket Preferences ===== ##
########################################
UNIVERSE_FROM_WS=1
WS_UNIVERSE_READY_WAIT_SEC=3.0
WS_PING_INTERVAL_SEC=20.0
USE_WS_CANDLES=1
WS_MAX_SYMBOLS=60
WS_BACKFIL_BARS=240

########################################
## ====== Universe Cache / Fallback == ##
########################################
UNIVERSE_CACHE_PATH=.universe_cache.json
UNIVERSE_TTL_SEC=900
FALLBACK_UNIVERSE=

########################################
## ====== Output / Limits ============
########################################
PRINT_LIQUIDITY_DEBUG=0
PRINT_DIAGNOSTICS=0
PRINT_JSON_LINES=0
MAX_CANDIDATES_PER_TF=8
MAX_CANDIDATES_PER_LOOP=24

